{
  "name": "template-ts",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "start": "sucrase-node src/server.ts",
    "test": "SET NODE_ENV=test& jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "SET NODE_ENV=test& jest --watchAll --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watchAll -c jest-unit-config.js",
    "test:integration": "npm test -- --watchAll -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "debug": "nodemon --inspect src/server.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
